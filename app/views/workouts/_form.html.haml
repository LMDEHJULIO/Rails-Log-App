= simple_form_for(@workout, html:{ class: 'form-horizontal'}) do |f|
	= f.input :date, label: "Date"
	= f.input :workout, label: "Workout workout did you do?", input_html: {class: "form-control"}
	= f.input :mood, label: "How did you feel?", input_html: {class: "form-control"}
	= f.input :length, label: "How long was your workout", input_html: {class: "form-control"}
	%br/
	= f.button :submit